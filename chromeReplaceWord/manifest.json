{
    "manifest_version": 3,
    "name": "Szócsere Bővítmény",
    "version": "1.0",
    "description": "Egy egyszerű Chrome bővítmény, ami kicseréli a megadott szavakat az általad beállított szóra.",
    "action": {
      "default_popup": "options.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
  }
  